<div>
    <h1>Login</h1>

    <%= form_for :sessions, url: login_path do |f| %>
        <%= f.label :email%>
        <%= f.email_field :email%>

        <%= f.label :password%>
        <%= f.password_field :password%>

        <%= f.submit "Log In"%>
    <% end %>
    <br>
    <%= link_to 'Create New Account', new_user_path %>
</div>